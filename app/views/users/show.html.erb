<div class="info">
    <% if @user.admin %>
      <p><strong>ADMIN</strong></p>
    <% end %>
<h1><%= @user.name %></h1>
<% if !@user.admin %>
<h2>Tickets: <%= @user.tickets %></h2>
<h2>Height: <%= @user.height %></h2>
<h2>Mood: <%= @mood %></h2>
<% end %>
<p><%= link_to 'See attractions', attractions_path %></p>
<a href="/logout">Log Out</a>